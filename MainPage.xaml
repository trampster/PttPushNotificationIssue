<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:viewmodels="clr-namespace:PttPushNotificationIssue;assembly=PttPushNotificationIssue"
             x:DataType="viewmodels:PttPushViewModel"
             x:Class="PttPushNotificationIssue.MainPage">

    <ScrollView>
        <VerticalStackLayout
            Padding="30,0"
            Spacing="25">
            <Label Text="APNS Token:" />
            <Label Text="{Binding ApnsToken}" />

            <Label Text="Pushes Received:" />
            <Label Text="{Binding Log}" />

        </VerticalStackLayout>
    </ScrollView>

</ContentPage>
